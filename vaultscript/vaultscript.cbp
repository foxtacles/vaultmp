<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="vaultscript" />
		<Option platforms="Windows;Unix;" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Windows">
				<Option platforms="Windows;" />
				<Option output="vaultscript" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Windows/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wall" />
					<Add option="-DBUILD_DLL" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
			<Target title="Unix">
				<Option platforms="Unix;" />
				<Option output="vaultscript" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Unix/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wall" />
					<Add option="-DBUILD_DLL" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Linker>
			<Add option="-static-libgcc" />
			<Add option="-static-libstdc++" />
			<Add option="--no-export-all-symbols" />
			<Add option="--add-stdcall-alias" />
		</Linker>
		<Unit filename="vaultscript.cpp" />
		<Unit filename="vaultscript.h" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
